<div class="page-header">
  <h1>All Profiles</h1>
  <%= link_to "New Profile", new_profile_path %> | 

</div>


<% @profiles.each do |profile| %>
<div class="session-full">
  <% @user = profile.user %>
  <br>
  <ul>
    <li><strong><%= profile.first_name %></strong></li>
    <li><p>Name: <%= profile.last_name %></p></li>
    <li><p><%= profile.about_me %></p></li>

    <%#= link_to time_ago_in_words(profile.created_at) + " ago", profile_path(@user) %> 

    <li><span>
      <%= link_to "Show", profile %>
    </span> </li>
  </ul>
  <br>


</div>
<% end %>


