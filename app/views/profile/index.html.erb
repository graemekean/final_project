<div class="page-header">
  <h1>All Profiles</h1>
</div>


<% @profiles.each do |profile| %>
<div class="session-full">
  <strong><%= profile.first_name %></strong>
  <p>Name: <%= profile.last_name %></p>
  <p><%= profile.about_me %></p>

  <%= link_to time_ago_in_words(profile.created_at) + " ago", profile %> | 

  <span class="session-control">
    <%= link_to "Show", profile %> | 
    <%= link_to "Edit", edit_profile_path(profile) %> | 
    <%= link_to "Delete", profile, method: :delete, data: {confirm: "Are you sure you want to delete this profile? Doing so will clear your profile and make you unavailable to your friends"}  %>
  </span> 



  <!-- <p><%#= session.user %> -->
</div>
<% end %>

    <%= link_to "New Profile", new_profile_path %> | 




