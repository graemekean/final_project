<div class="page-header">
  <h1>All of all the groups in the system</h1>
  <%= link_to 'New Group', new_group_path %>
</div>




<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

<% @groups.each do |group| %>
<% index = 1 %>
<% text = index.to_words %>

  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="headingOne">
      <h4 class="panel-title">
        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
          <%= group.title %>
        </a>
      </h4>
    </div>
    <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
      <div class="panel-body">
        <p>Members:
        <% group.group_memberships.each do |membership| %>
        <%= membership.user.email %>, 
        <% end %></p>
        <p>Description: <%= group.description %></p>
        <p><%= group.creator %>
        <% group.group_memberships.each do |member| %>
        <%= member.user.email %>
        <% end %>


        <%= "created " + time_ago_in_words(group.created_at) + " ago" %> | 

        <span class="session-control">
          <%= link_to "Show", group %> | 
          <%= link_to "Edit", edit_group_path(group) %> | 
          <%= link_to "Delete", group, method: :delete, data: {confirm: "Are you sure you want to delete this group?"}  %>
        </span> 
      </div>
    </div>
  </div>

  <% end %>
</div>



