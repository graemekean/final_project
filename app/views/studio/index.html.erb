<div class="page-header">
  <h1>My studio</h1>
  <%= link_to 'New Studio', new_studio_path %>
  
</div>



<% @studios.each do |studio| %>
<div class="session-full">
  <strong><%= studio.status %></strong>
  <p>Name: <%= studio.user_id %></p>
  <p><%= studio.user %></p>

  <%= link_to time_ago_in_words(studio.created_at) + " ago", studio %> | 

  <span class="session-control">
    <%= link_to "Show", studio %> | 
    <%= link_to "Edit", edit_studio_path(studio) %> | 
    <%= link_to "Delete", studio, method: :delete, data: {confirm: "Are you sure you want to delete this studio? Doing so will also delete any projects within it and all their associated audio files."}  %>
  </span> 



  <!-- <p><%#= session.user %> -->
</div>
<% end %>
<% else %>




