<p id="notice"><%= notice %></p>

<div class="page-header">
  <h1>All of the sessions</h1>
  <%= link_to 'New Session', new_session_path %>
</div>

<% @sessions.each do |session| %>
<div class="container">
  <strong><%= session.title %></strong>
  <p><%#= session.title %></p>
  <p><%= session.description %></p>

  <%= link_to time_ago_in_words(session.created_at) + " ago", session %> | 

  <span class="session-control">
    <%= link_to "Show", session %> | 
    <%= link_to "Edit", edit_session_path(session) %> | 
    <%= link_to "Delete", session, method: :delete, data: {confirm: "Are you sure you want to delete this session? Doing so will also delete any files you recorded in the session."}  %>
  </span> 



  <!-- <p><%#= session.user %> -->
</div>
<% end %>


<br>


