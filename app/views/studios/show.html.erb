<div class="page-header">
  <h1>My studio</h1>
  <%= link_to 'New Project', new_project_path %>

</div>



<% @studio.each do |studio| %>
<div class="session-full">
  <strong><%= studio.user.email %></strong>
  <p>Name: <%= studio.user.status %></p>
  <p><%= project.description %></p>

  <%= link_to time_ago_in_words(project.created_at) + " ago", project %> | 

  <span class="session-control">
    <%= link_to "Show", studio %> | 
    <%= link_to "Edit", edit_studio_path(studio) %> | 
    <%= link_to "Delete", studio, method: :delete, data: {confirm: "Are you sure you want to delete this studio? Doing so will also delete any projects contained within it."}  %>
  </span> 



  <!-- <p><%#= session.user %> -->
</div>
<% end %>
<% else %>




